.music-controller-container {
  width: 100vw;
  position: -webkit-sticky;
  position: sticky;
  left: 0;
  bottom: 0;
  padding-bottom: 20px;
}
.music-controller-container .simplify-controller {
  width: 90%;
  height: 50px;
  border-radius: 25px;
  margin: 0 auto;
  position: relative;
  display: flex;
}
.music-controller-container .simplify-controller .controller-song-list {
  position: absolute;
  width: 100%;
  height: 50vh;
  left: 0;
  bottom: 50%;
  border-radius: 15px 15px 0 0;
  background-color: #9acafc;
  opacity: 1;
  padding-bottom: 25px;
}
.music-controller-container .simplify-controller .con-bj {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  border-radius: 25px;
  overflow: hidden;
  background: linear-gradient(145deg, #ffffff, #e6e6e6);
  box-shadow: 5px 5px 10px #b8b8b8, -5px -5px 10px #ffffff;
  transition: 1s;
}
.music-controller-container .simplify-controller .con-bj > image {
  width: 101%;
  height: 100%;
  -webkit-filter: blur(10px);
          filter: blur(10px);
  transition: 1s;
}
.music-controller-container .simplify-controller .controller-cover {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 60px;
  height: 60px;
  background-color: #909399;
  overflow: hidden;
  border-radius: 5px;
}
.music-controller-container .simplify-controller .controller-cover > image {
  width: 100%;
  transition: 1s;
}
.music-controller-container .simplify-controller > .lyric-box {
  width: 55%;
  height: 100%;
  z-index: 1;
  margin-left: 60px;
  display: flex;
  align-items: center;
  padding-left: 10px;
  position: relative;
  overflow: hidden;
}
.music-controller-container .simplify-controller > .lyric-box > text {
  position: absolute;
  display: inline-block;
  width: calc(100% - 10px);
  height: 100%;
  color: #f4f4f5;
  transition: .2s;
  left: 10px;
  top: -200%;
  display: flex;
  align-items: center;
  opacity: 0;
}
.music-controller-container .simplify-controller > .control-btn-box {
  width: calc(45% - 60px);
  z-index: 1;
  display: flex;
}
.music-controller-container .simplify-controller > .control-btn-box > view {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
}
.music-controller-container .simplify-controller > .control-btn-box > view:first-child > view {
  height: 60%;
  aspect-ratio: 1/1;
  border: 2px solid #fff;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #fff;
}
.music-controller-container .simplify-controller > .control-btn-box > view:first-child > view > ._i {
  font-size: 18px;
}
.music-controller-container .simplify-controller > .control-btn-box > view:first-child > view > .play-icon {
  margin-left: 2px;
}
.music-controller-container .simplify-controller > .control-btn-box > view > ._i {
  font-size: 18px;
  color: #fff;
}
.lrcShow {
  transition: 1.5s !important;
  opacity: 1 !important;
  top: 0 !important;
}
.lrcHidden {
  transition: .5s;
  opacity: 0;
}
.info {
  display: block;
  -webkit-animation: info .5s;
          animation: info .5s;
  pointer-events: auto;
}
.out {
  -webkit-animation: out .5s;
          animation: out .5s;
  -webkit-animation-fill-mode: forwards;
          animation-fill-mode: forwards;
  pointer-events: none;
}
@-webkit-keyframes info {
0% {
    bottom: 0;
    opacity: 0;
}
100% {
    bottom: 50%;
    opacity: 1;
}
}
@keyframes info {
0% {
    bottom: 0;
    opacity: 0;
}
100% {
    bottom: 50%;
    opacity: 1;
}
}
@-webkit-keyframes out {
0% {
    bottom: 50%;
    opacity: 1;
    display: block;
}
100% {
    bottom: 0;
    opacity: 0;
    display: none;
}
}
@keyframes out {
0% {
    bottom: 50%;
    opacity: 1;
    display: block;
}
100% {
    bottom: 0;
    opacity: 0;
    display: none;
}
}
@-webkit-keyframes lrc {
0% {
    top: 0 !important;
}
20% {
    top: 100% !important;
}
95% {
    top: 100% !important;
}
100% {
    top: 0 !important;
}
}
@keyframes lrc {
0% {
    top: 0 !important;
}
20% {
    top: 100% !important;
}
95% {
    top: 100% !important;
}
100% {
    top: 0 !important;
}
}

